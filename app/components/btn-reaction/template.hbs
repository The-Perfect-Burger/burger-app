{{#if (eq reaction "loved")}}
  <img class="icon icons8-Like" src="data:image/png;base64,"
  width="{{size}}" height="{{size}}">
  Loved
{{/if}}
{{#if (eq reaction "liked")}}
  <img class="icon icons8-Like-It" src="data:image/png;base64,"
    width="{{size}}" height="{{size}}">
  Liked
{{/if}}
{{#if (eq reaction "disliked")}}
  <img class="icon icons8-Like-It flip-y" src="data:image/png;base64,"
    width="{{size}}" height="{{size}}">
  Disliked
{{/if}}
{{#if (eq reaction "tried")}}
  <img class="icon icons8-Ok" src="data:image/png;base64,"
    width="{{size}}" height="{{size}}">
  Tried
{{/if}}
